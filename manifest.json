{
  "manifest_version": 2,
  "name": "KernelCodeViewer",
  "description": "Get Kernel notes and fill page with KernelCodeViewer by url",
  "version": "1.0",
  "page_action": {
    "default_icon": "icon.png",
    "default_title": "on"
  },
  "content_scripts": [
    {
      "matches": ["*://elixir.bootlin.com/*","*://xr-hulk-k8s-node1931.gh.sankuai.com/*"],
      "js": ["popup.js"]
    }
  ],
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
